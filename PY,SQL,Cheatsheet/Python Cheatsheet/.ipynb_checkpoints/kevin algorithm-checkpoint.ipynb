    "#this is algo to calculate and summarize point between 2 list\n",
    "point_a=[1,2,1]\n",
    "point_b=[2,1,1]\n",
    "def score(a,b):\n",
    "    result = []\n",
    "    ap=0\n",
    "    bp=0\n",
    "    for c,d in zip(point_a,point_b):\n",
    "        if c>d:\n",
    "            ap +=1\n",
    "        elif c==d:\n",
    "            pass\n",
    "        else:\n",
    "            bp+=1\n",
    "    result+=[ap,bp]\n",
    "    return result\n",
    "\n",
    "\n",
    "score(point_a,point_b)"
   ]
